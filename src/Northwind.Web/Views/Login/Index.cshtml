@model IList<Northwind.Web.Controllers.LoginController.LoginModel>

@{
    ViewBag.Title = "Login";
}

<div class="page-header">
    <h1>Login <small>run as user</small></h1>
</div>

<form role="form" class="form-inline" action="@Url.Action("Index", "Login")" method="post">
    <div class="form-group">
        <label for="id">Choose a user</label>
        <select class="form-control" name="id">
            @foreach (var m in Model)
            {
            <option value="@m.Id">@m.Name</option> }
        </select>
    </div>
    <button type="submit" class="btn btn-default btn-primary">Go</button>
</form>


