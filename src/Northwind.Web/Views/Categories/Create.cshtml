@model Northwind.Web.InputModels.CategoryInputModel

@{
    ViewBag.Title = "Create Category";
}

<div class="row">
    <div class="page-header">
        <h1>Create Category<small></small></h1>
    </div> 
</div>

<div class="row">
    <div class="col-lg-12">
        @Html.ValidationSummary(true)
    </div>
</div>

<div class="row">
    <div class="col-lg-8">
        <form action="@Url.Action("Create")" method="post" class="form-horizontal" role="form">
            @Html.Partial("_Form", Model)
        </form>
    </div>
</div>
